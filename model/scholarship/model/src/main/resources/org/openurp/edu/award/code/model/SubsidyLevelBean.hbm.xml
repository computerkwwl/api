<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.edu.awardp.code.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
   
    <class name="SubsidyLevelBean" entity-name="org.openurp.edu.award.code.SubsidyLevel">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="long"><generator class="date"/></id>
        <property name="code" length="40" unique="true" not-null="true"/>
        <property name="name" length="80" not-null="true"/>
        <property name="enabled" not-null="true"/>
        <property name="discription"/>
        <property name="awardAmount"/>
        <many-to-one name="subsidyCategory" not-null="true"/>
    </class>
</hibernate-mapping>